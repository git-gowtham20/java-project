pipeline {
    agent any
    environment {
        SCANNER_HOME = tool 'sonarqube-tool'
    }
        stage('code analysis') {
            steps {
              withSonarQubeEnv('sonarqube-server') {
               sh ''' $SCANNER_HOME/bin/sonar-scanner -Dsonar.projectName=java-project \
               -Dsonar.java.binaries=. \
               -Dsonar.projectKey=java-project'''
              }
            }
        }
        stage('build') {
            steps {
                sh 'mvn clean install'
            }
        }

        stage('docker push') {
         steps {
         withDockerRegistry(credentialsId: 'docker-cred') {
             sh 'docker tag java-app2 git-gowtham20/java-app2'
             sh 'docker push gow9629608170/java-app2'
          }
          }
         } 
        stage('image') {
            steps {
                sh 'docker build -t project .'
            }
        }
        stage('container') {
            steps {
                sh 'docker run -itd --name java11 -p 8086:8080 project'
            }
            
        }
        
    }
}
